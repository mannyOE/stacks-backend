        "@types/compression": "^1.7.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/faker": "^4.1.12",
        "@types/helmet": "0.0.47",
        "@types/lodash": "^4.14.153",
        "@types/mocha": "^7.0.2",
        "@types/morgan": "^1.9.0",
        "@types/sinon": "^9.0.4",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
