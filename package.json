        "ts-node": "^8.10.1",
        "@types/mongoose": "^5.7.20",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/express-validator": "^3.0.0",
        "@types/node": "^14.0.1",
        "typescript": "^3.9.2"
    },
    "devDependencies": {
