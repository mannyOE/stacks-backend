{
    "name": "stax-ave",
    "version": "1.0.0",
    "private": true,
    "description": "Staxave is an entertainment streaming service",
    "main": "src/index.ts",
    "scripts": {
        "prestart": "npm run build",
        "start": "node -r ./tsconfig-path-bootstrap.js ./dist",
        "dev": "npx nodemon -e ts --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
        "lint": "eslint --ext .ts,.json --max-warnings=0 src",
        "test": "NODE_ENV=test nyc mocha -r ts-node/register -r tsconfig-paths/register src/**/*.spec.ts",
        "test:unit": "NODE_ENV=test nyc mocha -r ts-node/register -r tsconfig-paths/register --grep @unit src/**/*.spec.ts",
        "build": "rm -rf ./dist && npm i && npx tsc && cp -r ./src/utils/templates ./dist/utils/templates && rm -rf ./node_modules/ && npm i --only=prod",
        "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
        "deploy": "gh-pages -d docs",
        "cool": "node --inspect=5548 -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
        "cool:watch": "nodemon"
    },
    "keywords": [
        "typescript",
        "jsdoc",
        "express",
        "eslint",
        "bootstrap"
    ],
    "author": "Ahman Emmanuel <ahmanemmanuel@gmail.com>",
    "license": "ISC",
    "dependencies": {
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node-mailjet": "^3.3.3",
        "@types/request-promise": "^4.1.46",
        "bcrypt": "^4.0.1",
        "bluebird": "^3.7.2",
        "fs-extra": "^9.1.0",
        "body-parser": "^1.19.0",
        "cloudinary": "^1.23.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.0",
        "express-validator": "^6.5.0",
        "firebase-admin": "^8.13.0",
        "googleapis": "^52.1.0",
        "hbs": "^4.1.1",
        "helmet": "^3.22.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.15",
        "moment": "^2.27.0",
        "mongoose": "^5.9.15",
        "mongoose-paginate": "^5.0.3",
        "morgan": "^1.10.0",
        "node-input-validator": "^4.2.1",
        "node-mailjet": "^3.3.1",
        "paystack-api": "^2.0.5",
        "request": "^2.88.2",
        "request-promise": "^4.2.5",
        "tsconfig-paths": "^3.9.0",
